{
  "name": "@celery/ui-components",
  "version": "2.0.0-10",
  "description": "Shared UI Components and Styles",
  "main": "components/index.less",
  "scripts": {
    "test": "npm run build",
    "build": "rm -rf build && broccoli build build",
    "build-watch": "broccoli-timepiece build",
    "styleguide": "styledown components/**/*.less styleguide/config.md > styleguide/index.html",
    "styleguide-watch": "watch 'npm run styleguide' components mixins styleguide --wait=1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/airbrite/ui-components.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/airbrite/ui-components/issues"
  },
  "homepage": "https://github.com/airbrite/ui-components#readme",
  "dependencies": {},
  "devDependencies": {
    "broccoli": "^0.16.4",
    "broccoli-cli": "^1.0.0",
    "broccoli-funnel": "^0.2.3",
    "broccoli-less-single": "^0.4.0",
    "broccoli-merge-trees": "^0.2.2",
    "broccoli-timepiece": "^0.3.0",
    "normalize.css": "^3.0.3",
    "styledown": "^1.0.2",
    "watch": "^0.16.0"
  }
}
